[flake8]
max-line-length = 127
max-complexity = 15
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    .pytest_cache,
    htmlcov,
    .coverage,
    .mypy_cache,
    .tox,
    build,
    dist,
    *.egg-info
ignore = 
    E203,
    W503,
    E501,
per-file-ignores =
    __init__.py:F401
